pipeline{
    agent any
    stages{
        stage('Test Ubuntu'){
            steps{
                echo "Test Ubuntu"
                sleep 10
            }
        }
        stage('Test for windows'){
            steps{
                echo "Test Windows"
                sleep 20
            }
        }
        stage('Test extra OS'){
            steps{
                echo "Test extea OS"
                sleep 5
            }
        }
    }
}
